canadianPostalCode {
  exp              = (firstAlpha + digit approvedAlpha + space +  digit + approvedAlpha + digit)              --code
  firstAlpha       = ~badFirst upper
  approvedAlpha    = ~badAlpha upper 
  badAlpha         = "D" | "F" | "I" | "O" | "Q" | "U"
  badFirst         = badAlpha | "W" | "Z"
}

